{
	"version": 2,
	"builds": [
	  {
		"src": "index.js",
		"use": "@now/node"
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "index.js"
	  }
	],
	"functions": {
	  "api/*": {
		"runtime": "@now/node",
		"memory": 512
	  }
	},
	"dependencies": {
	  "express": "^4.18.2",
	  "cors": "^2.8.5"
	}
  }
  